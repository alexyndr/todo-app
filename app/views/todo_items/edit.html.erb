<div class="container">

	<b>Edit TODOS</b>

		<%= form_with model: ([@todo_list, @todo_item]) do |f| %>
			<%= f.text_field :content, placeholder: "Edit Todoss" %>
			<%= f.date_select :deadline, start_year: DateTime.now.year, default: 3.days.from_now  %>
			<%= f.submit %>
		<% end %>

	<div class="links">
		<%= link_to 'Back', user_todo_lists_path(current_user) %>
	</div>
</div>